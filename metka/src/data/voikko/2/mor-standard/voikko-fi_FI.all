subrule sisar ($tietue, $vahva_aste, $heikko_aste):
  return taivutuskaava ($tietue,
                        yksikkö8 ($vahva_aste + "e", $heikko_aste,
                                  $vahva_aste + "e", $vahva_aste + "e",
                                  [osanto_tA: $heikko_aste],
