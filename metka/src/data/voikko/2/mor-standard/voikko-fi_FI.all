                         johdin_mAinen:   "psi",
                         johdin_inen:     "ps",

                         nimentö_t:        "pse",
                         omanto_ten:       "s",
                         omanto_ien:       "ps",
                         osanto_iA:        "ps",
                         olento_inA:       "ps",
                         sija_monikko_1:     "ps",
                         sisätulento_iin:  "ps",
                         ulkopaikallissija_illA: "ps",
                         vajanto_ittA:     "ps",
                         seuranto_ine:     "ps",
                         johdin_ittAin:    "ps"]);
end;


subrule veitsi ($tietue):
  return taivutuskaava ($tietue,
                        yksikkö8 ("tse", "tsi", "tse", "tse",
