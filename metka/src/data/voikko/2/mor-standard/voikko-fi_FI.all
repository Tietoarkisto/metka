subrule rV ($tietue):
  if ($tietue.alku matches ".*ier") then
    return ("r" + O($tietue));
  elseif ($tietue.alku matches ".*[^i]er") then
    return ("r" + U($tietue));
  elseif ($tietue.alku matches ".*[aiä]r") then
    return ("r" + U($tietue));
  elseif ($tietue.alku matches ".*[ouyö]r") then
    return ("r" + O($tietue));
  else
    error "subrule rV: väärä sana " + $tietue.alku + " " + $tietue.perusmuoto;
  end;
end;


